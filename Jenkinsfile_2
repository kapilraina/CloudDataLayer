node {

	stage('Preparation') {  
 
     	checkout scm
      
 
   }
   
   stage('Build Code') {  
 
     	sh "mvn clean package"
 
   }
   
    stage('Build Code') {  
 
     	def customImage = docker.build("kapilshekharraina/clouddatalayer:${env.BUILD_ID}")
 
   }
    
     stage('Push Image') {  
	    docker.withRegistry("https://docker.io","DockerHub"){
		  	
		    customImage.push('latest')
	    }
    }
}